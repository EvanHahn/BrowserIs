{
  "name": "browseris",
  "author": "Evan Hahn <me@evanhahn.com>",
  "version": "1.1.0",
  "description": "Ask questions about the browser in Express.",
  "keywords": ["browser", "useragent", "detection", "mobile", "tablet", "desktop"],
  "repository": {
    "type": "git",
    "url": "git://github.com/EvanHahn/BrowserIs.git"
  },
  "jshintConfig": {
    "bitwise": true,
    "camelcase": true,
    "curly": false,
    "eqeqeq": false,
    "es3": false,
    "freeze": true,
    "immed": true,
    "latedef": true,
    "newcap": true,
    "noarg": true,
    "noempty": true,
    "nonew": true,
    "plusplus": false,
    "quotmark": true,
    "undef": true,
    "unused": true,
    "strict": false,
    "trailing": true,
    "node": true,
    "predef": [
      "describe", "it", "beforeEach"
    ]
  },
  "main": "./browseris",
  "license": "MIT"
}